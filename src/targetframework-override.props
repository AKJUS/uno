<Project>
	<PropertyGroup>
		<_ShouldUseNoPlatformOverride>false</_ShouldUseNoPlatformOverride>
		<_ShouldUseNoPlatformOverride Condition="$(MSBuildProjectFile.EndsWith('.Skia.csproj')) OR $(MSBuildProjectFile.EndsWith('.Wasm.csproj')) OR $(MSBuildProjectFile.EndsWith('.Reference.csproj'))">true</_ShouldUseNoPlatformOverride>
	</PropertyGroup>

	<Import Condition="'$(_ShouldUseNoPlatformOverride)'=='true'" Project="targetframework-override-noplatform.props" />
	<PropertyGroup Condition="'$(UnoTargetFrameworkOverride)'!='' AND '$(_ShouldUseNoPlatformOverride)' == 'false'">
		<TargetFrameworks>$(UnoTargetFrameworkOverride)</TargetFrameworks>
		<TargetFrameworks Condition="'$(TargetFrameworks)'=='net8.0-ios' and '$(OutputType)'!='Exe'">net8.0-ios17.0</TargetFrameworks>
		<TargetFrameworks Condition="'$(TargetFrameworks)'=='net9.0-ios' and '$(OutputType)'!='Exe'">net9.0-ios17.2</TargetFrameworks>
		<TargetFrameworks Condition="'$(TargetFrameworks)'=='net8.0-maccatalyst' and '$(OutputType)'!='Exe'">net8.0-maccatalyst17.0</TargetFrameworks>
		<TargetFrameworks Condition="'$(TargetFrameworks)'=='net9.0-maccatalyst' and '$(OutputType)'!='Exe'">net9.0-maccatalyst17.2</TargetFrameworks>
		<TargetFrameworks Condition="'$(TargetFrameworks)'=='net8.0-macos' and '$(OutputType)'!='Exe'">net8.0-macos14.0</TargetFrameworks>
		<TargetFrameworks Condition="'$(TargetFrameworks)'=='net9.0-macos' and '$(OutputType)'!='Exe'">net9.0-macos14.2</TargetFrameworks>
	</PropertyGroup>

</Project>
